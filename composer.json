        "symfony/http-foundation": "~2.8|~3.0",
        "symfony/security": "~3.0|~4.0",
        "symfony/serializer": "~3.0|~4.0",
        "symfony/validator": "~3.0|~4.0",
        "symfony/yaml": "~3.0|~4.0",
        "szymach/c-pchart": "~3.0",
        "thenetworg/oauth2-azure": "^1.4",
