msgstr "Փորձարկում/զարգացում"

msgid "Crons"
msgstr "Կրոններ"

msgid "Editor"
msgstr "Նշիչ"

msgid "Gradebook"
msgstr "Վկայական"

msgid "Internationalization"
msgstr "Ինտերնացիոնալիզացիա"

