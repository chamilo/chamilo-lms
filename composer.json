        "sonata-project/core-bundle": "~3.1|~4.0",
        "sonata-project/user-bundle": "~3.0|~4.0",
        "stripe/stripe-php": "*",
        "studio-42/elfinder": "2.1.*",
        "sunra/php-simple-html-dom-parser": "~1.5.0",
        "sylius/attribute": "0.13.0",
