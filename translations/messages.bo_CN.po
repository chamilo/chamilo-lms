msgid "Cancelled"
msgstr "བསྲུངས་པ"

msgid "Finished"
msgstr "མཇུག་སྒྲིལ།"

msgid "Session status"
msgstr "བསྡད་སྐོར་གྱི་དོན་སྙོམས།"

msgid "Update session status"
msgstr "བསྡད་སྐོར་གྱི་དོན་སྙོམས་གསར་བསྡད།"

