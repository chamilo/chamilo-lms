{% extends "@ChamiloTheme/Layout/layout_one_col.html.twig" %}
{% block content %}

{% autoescape false %}
<div class="summary-legend">
    {{ table }}
</div>
<script>
    $(function() {
        $('.easypiechart-blue').easyPieChart({
            scaleColor: false,
            barColor: '#30a5ff',
            lineWidth:8,
            trackColor: '#f2f2f2'
        });
    });
    $(function() {
        $('.easypiechart-red').easyPieChart({
            scaleColor: false,
            barColor: '#f9243f',
            lineWidth:8,
            trackColor: '#f2f2f2'
        });
    });
</script>

{% endautoescape %}
{% endblock %}